{
    "name": "fluentdom/html5",
    "type": "library",
    "description": "HTML5 support for FluentDOM",
    "keywords": [
        "xml", "dom", "fluentdom", "html5"
    ],
    "license": "MIT",
    "homepage": "http://fluentdom.org",
    "authors": [
        {
            "name": "FluentDOM Contributors",
            "homepage": "https://github.com/FluentDOM/FluentDOM/contributors"
        }
    ],
    "config": {
        "platform": {
            "php": "7.0"
        }
    },
    "require": {
        "php": ">=7.0",
        "fluentdom/fluentdom": "7.x",
        "masterminds/html5": "~2.2"
    },
    "require-dev": {
      "fluentdom/selectors-phpcss": "^1.1",
      "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "files" : ["src/plugin.php"],
        "psr-4": {
            "FluentDOM\\HTML5\\": "src/"
        }
    }
}
